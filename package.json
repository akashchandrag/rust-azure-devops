{
  "name": "rust-vsts",
  "version": "1.0.0",
  "description": "Rust extension for Visual Studio Team Services",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'tasks/**/*.ts'",
    "compile": "tsc",
    "add-sdk-to-install-task": "npm install vsts-task-lib --prefix ./tasks/install",
    "add-sdk-to-cargo-task": "npm install vsts-task-lib --prefix ./tasks/cargo",
    "package": "tfx extension create --manifest-globs vss-extension.json --output-path dist",
    "publish": "tfx extension publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spontoreau/rust-vsts.git"
  },
  "keywords": [
    "rust",
    "rust language",
    "cargo",
    "build",
    "vsts",
    "ci",
    "continious integration"
  ],
  "author": "Sylvain Pontoreau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spontoreau/rust-vsts/issues"
  },
  "homepage": "https://github.com/spontoreau/rust-vsts#readme",
  "dependencies": {
    "vsts-task-lib": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.1",
    "@types/q": "^1.5.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "run-sequence": "^2.2.1",
    "tfx-cli": "^0.5.12",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  }
}
