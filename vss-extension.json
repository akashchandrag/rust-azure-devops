{
    "manifestVersion": 1,
    "id": "rust",
    "version": "0.2.21",
    "name": "Rust",
    "description": "Rust extension for Visual Studio Team Services",
    "publisher": "spontoreau",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "icon.png"
    },
    "scopes": [
        "vso.work"
    ],
    "files": [
        {
            "path": "tasks/cargo"
        },
        {
            "path": "tasks/install"
        }
    ],
    "contributions": [
        {
            "id": "rust-install",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/install"
            }
        },
        {
            "id": "rust-cargo",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/cargo"
            }
        }
    ]
}